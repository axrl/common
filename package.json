{
  "name": "@axrl/common",
  "author": {
    "name": "Axrl",
    "email": "axrl@mail.ru"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:axrl/common.git"
  },
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "upd": "npm update --save --force --legacy-peer-deps",
    "build:form-builder": "ng build ngx-extended-form-builder -c production",
    "watch:form-builder": "ng build ngx-extended-form-builder --watch --configuration development",
    "gen-doc:form-builder": "typedoc --tsconfig projects/ngx-extended-form-builder/tsconfig.lib.prod.json",
    "pub:form-builder": "node updatePackageVersion.js ./projects/ngx-extended-form-builder/package.json && ng build ngx-extended-form-builder -c production && cd dist/ngx-extended-form-builder && npm publish",
    "build:common": "ng build common -c production",
    "watch:common": "ng build common --watch --configuration development",
    "gen-doc:common": "typedoc --tsconfig projects/common/tsconfig.lib.prod.json",
    "pub:common": "node updatePackageVersion.js ./projects/common/package.json && ng test common && ng build common -c production && cd dist/common && npm publish",
    "build:one-table": "ng build one-table -c production",
    "watch:one-table": "ng build one-table --watch --configuration development",
    "gen-doc:one-table": "typedoc --tsconfig projects/one-table/tsconfig.lib.prod.json",
    "pub:one-table": "node updatePackageVersion.js ./projects/one-table/package.json && ng build one-table -c production && cd dist/one-table && npm publish",
    "test:common": "ng test common --code-coverage",
    "test:form-builder": "ng test ngx-extended-form-builder --code-coverage",
    "update-and-publish:form-builder": "npm run upd && npm run gen-doc:form-builder && npm run pub:form-builder",
    "update-and-publish:common": "npm run upd && npm run gen-doc:common && npm run pub:common",
    "update-and-publish:one-table": "npm run upd && npm run gen-doc:one-table && npm run pub:one-table"
  },
  "homepage": "https://github.com/axrl/common",
  "private": false,
  "dependencies": {
    "@angular-devkit/architect": "^0.1502.9",
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "^15.2.10",
    "@axrl/common": "^1.0.40",
    "@axrl/ngx-extended-form-builder": "^1.3.1",
    "listr": "^0.14.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@types/jasmine": "~4.3.6",
    "@types/listr": "^0.14.9",
    "@types/node": "^18.19.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "~8.9.0",
    "eslint-plugin-import": "~2.28.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-rxjs": "^5.0.3",
    "jasmine-core": "^5.1.2",
    "karma": "6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^15.2.2",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "~4.9.4"
  }
}
